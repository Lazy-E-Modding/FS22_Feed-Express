<?xml version="1.0" encoding="utf-8" standalone="no"?>
<vehicle type="trailer" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://validation.gdn.giants-software.com/xml/fs22/1.6.0.0/vehicle.xsd">
  <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>
  <storeData>
    <name>Lazy E Feed Express Cake Box M+</name>
    <specs>
    </specs>
    <functions>
      <function>$l10n_function_feeder</function>
    </functions>
    <image>store.dds</image>
    <price>4000</price>
    <lifetime>600</lifetime>
    <rotation>0</rotation>
    <brand>LAZY_E_MANUFACTURING</brand>
    <category>trailers</category>
    <shopTranslationOffset>0 0.5 0</shopTranslationOffset>
    <shopRotationOffset>0 0 0</shopRotationOffset>
    <shopLoadingDelay initial="0.3" />
  </storeData>
  <base>
    <typeDesc>$l10n_typeDesc_mixerWagon</typeDesc>
    <filename>cakebox.i3d</filename>
    <size width="2" height="1" length="14.5" lengthOffset="0.5" />
    <speedLimit value="16" />
    <components>
      <component centerOfMass="0 0 0" solverIterationCount="40" mass="110" />
    </components>
    <schemaOverlay attacherJointPosition="0 0" name="IMPLEMENT" />
    <sounds volumeFactor="1.5" />
    <mapHotspot type="TRAILER" />
  </base>

  <attachable>
    <inputAttacherJoints>
      <inputAttacherJoint node="0>1|0" jointType="semitrailer" lowerRotLimitScale="0 0 0" upperRotLimitScale="0 0 0" upperRotationOffset="0" lowerTransLimitScale="0 0 0" upperTransLimitScale="0 0 0"/>
      <inputAttacherJoint node="0>1|0" jointType="semitrailerCar" lowerRotLimitScale="0 0 0" upperRotLimitScale="0 0 0" upperRotationOffset="0" lowerTransLimitScale="0 0 0" upperTransLimitScale="0 0 0"/>
    </inputAttacherJoints>
	<support animationName="moveSupport" />
  </attachable>
  
	<attacherJoints>
        <attacherJoint node="0>1|1" jointType="semitrailer" allowsJointLimitMovement="false" allowsLowering="false" lowerRotLimit="20 125 20">
            <schema position="1 0" rotation="0" invertX="false"/>
            <attachSound template="ATTACH_01" linkNode="0>"/>
        </attacherJoint>
    </attacherJoints>
	
	<cylindered>
		<movingTools>
			<movingTool node="0>1">
				<controls group="arm" axis="AXIS_FRONTLOADER_ARM" icon="FRONTLOADER_ARM_ROTATE" invertAxis="true"  mouseSpeedFactor="0.2"/>
				<translation transSpeed="0.7" transAcceleration="1.8" transMin="-5" transMax="5" translationAxis="3" />			
				<inputAttacherJoint value="true" />
			</movingTool>
		</movingTools>
	</cylindered>
  
  
  <animations>
    <animation name="empty">
      <part node="0>0|5" startTime="0" endTime="1" startRot="0 0 90" endRot="0 0 -27.76" />
    </animation>
	<animation name="toggleCover">
      <part node="0>0|2" startTime="0" endTime="1" startRot="0 0 0" endRot="90 0 0" />
    </animation>
	<animation name="moveSupport">
	</animation>
  </animations>
  
  <ai>
	<agentAttachment width="2" height="1" length="14.5" lengthOffset="0"/>
  </ai>
  
  <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration name="$l10n_configuration_valueDefault" price="0" >
                <fillUnits>
                    <fillUnit shopDisplayUnit="$l10n_unit_literShort" fillTypes="FERTILIZER LIME FORAGE FORAGE_MIXING DRYGRASS_WINDROW STRAW MINERAL_FEED PIGFOOD SILAGE SILOGRASS FORAGE2 FORAGE3 FORAGE4 FORAGE5 CHOPPEDMAIZE MINERAL_FEED CHOPPEDMAIZE_FERMENTED GRASS_FERMENTED CCM CCMRAW CHICKENFOOD FEEDPELLETS GRAINGRIST HORSEFOOD MINERALS PIGFOOD2 POWERFOOD SHEEPFOOD HAYPELLETS BEETPULP BEETPULP_FERMENTED POTATO_CUT BREWERSGRAIN BREWERSGRAIN_FERMENTED CARROT CROP_WINDROW WETGRASS_WINDROW SEMIDRYGRASS_WINDROW DRYCLOVER_WINDROW CLOVER_FERMENTED DRYALFALFA_WINDROW ALFALFA_FERMENTED DRYHORSEGRASS_WINDROW HORSEGRASS_FERMENTED" capacity="2000" >
                        <exactFillRootNode node="0>0|6|0|1" />
                    </fillUnit>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <fillVolume>
        <fillVolumeConfigurations>
            <fillVolumeConfiguration>
                <volumes>
                    <volume node="0>0|6|0|0" maxDelta="0.23" maxAllowedHeapAngle="18" fillUnitIndex="1" />
                </volumes>
            </fillVolumeConfiguration>
        </fillVolumeConfigurations>

        <unloadInfos>
            <unloadInfo>
                <node node="0>0|6|0|2" width="2" length="4.0" />
                <node node="0>0|6|0|3" width="2" length="1.0" minHeight="1.2" priority="2" />
            </unloadInfo>
        </unloadInfos>
    </fillVolume>

    <trailer>
        <trailerConfigurations>
            <trailerConfiguration name="$l10n_configuration_valueDefault" price="0">
                <trailer>
                    <tipSide name="$l10n_info_tipSideBack" dischargeNodeIndex="1">
                        <animation name="empty" speedScale="1.0" startTipTime="0.8"/>
                    </tipSide>
                </trailer>
            </trailerConfiguration>
        </trailerConfigurations>
    </trailer>

    <dischargeable>
        <dischargeNode node="0>0|6|1" emptySpeed="400" fillUnitIndex="1" unloadInfoIndex="1">
            <info width="1.0" length="0.5" zOffset="0.4" />
            <effects>
                <effectNode effectClass="TypedMotionPathEffect" effectType="MIXERWAGON" linkNode="0>0|6|3">
                    <motionPathEffect textureFilename="$data/vehicles/kuhn/ra142/unloadEffectFrontLeftArray.dds" numRows="8" rowLength="15" delay="0"/>
                </effectNode>
            </effects>
        </dischargeNode>
    </dischargeable>

<!--     <mixerWagon fillUnitIndex="1" recipe="FORAGE">

        <mixAnimationNodes>
            <animationNode node="0>0|0"   rotSpeed="25" rotAxis="1" turnOnFadeTime="1" turnOffFadeTime="1" />
        </mixAnimationNodes>
    </mixerWagon> -->

    <turnOnVehicle>
        <sounds>
            <work template="SILAGE_MIX_LOOP_01" linkNode="0>" />
            <start template="SILAGE_MIX_START_01" linkNode="0>" />
            <stop template="SILAGE_MIX_STOP_01" linkNode="0>" />
        </sounds>
    </turnOnVehicle>
	
	<cover>
        <coverConfigurations>
            <coverConfiguration>
                <cover openAnimation="toggleCover" fillUnitIndices="1 2" />
            </coverConfiguration>
        </coverConfigurations>
    </cover>




  <baseMaterial>
    <material name="mat_cakefedderMain" baseNode="0>0">
      <shaderParameter name="colorMat0" value="0 0 0" material="16"/>
    </material>
  </baseMaterial>

  <baseMaterialConfigurations useDefaultColors="true">
    <material name="mat_cakefedderMain" shaderParameter="colorMat0" />
      <baseMaterialConfiguration name="$l10n_ui_colorRed"            color="0.631 0.011 0.011"   material="40"/>
      <baseMaterialConfiguration name="$l10n_ui_colorBlack"         color="SHARED_BLACK2"  material="40"/>
      <baseMaterialConfiguration name="$l10n_ui_colorGrey"          color="SHARED_GREY1"   material="40"/>
	  <baseMaterialConfiguration name="$l10n_ui_colorWhite"            color="1 1 1"   material="40"/>
	  <baseMaterialConfiguration name="$l10n_ui_colorCream"            color="0.7372 0.6592 0.4423 1"   material="40"/>
      <baseMaterialConfiguration name="$l10n_ui_colorBlue"          color="SHARED_BLUE1"   material="40"/>
      <baseMaterialConfiguration name="$l10n_ui_colorYellow"            color="0.558 0.434 0.014"   material="40"/>
      <baseMaterialConfiguration name="$l10n_ui_colorGreen"            color="0.009 0.246 0.010"   material="40"/>
  </baseMaterialConfigurations>

  <wearable wearDuration="480" workMultiplier="5" fieldMultiplier="2" />
  <washable dirtDuration="90" washDuration="1" workMultiplier="4" fieldMultiplier="2" />
</vehicle>